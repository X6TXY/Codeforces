#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n;
    cin >> n;
    vector <string> a(n);
    for(int i=0;i<n;i++){
        int flag=0;
        cin >> a[i];
        for(int j=0;j<i;j++){
            if(a[i]==a[j]){
                flag=1;
                break;
            }
        }
        if(flag) cout << "YES"<<endl;
        else cout << "NO" << endl;
    }
}
