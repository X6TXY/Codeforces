#include <bits/stdc++.h>
 
using namespace std;
typedef long long ll;
#define rep(i, n) for(int i=0;i<n;i++)
int main(){
    int n,sum=0,cnt=0;
    cin >> n;
    int a[n][3];
    rep(i,n){
        for(int j=0;j<3;j++){
            cin >> a[i][j];
            sum +=a[i][j];
        }
        if(sum>=2) cnt++;
        sum=0;
    }
    cout << cnt;
}
